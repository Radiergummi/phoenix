{
  "opts":         {
    "recurse":             true,
    "encoding":            "utf8",
    "destination":         "docs",
    "template":            "templates/default"
  },
  "plugins":      [
  ],
  "recurseDepth": 10,
  "source":       {
    "include":        [
      "lib"
    ],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType":   "module",
  "tags":         {
    "allowUnknownTags": true,
    "dictionaries":     [
      "jsdoc",
      "closure"
    ]
  },
  "templates":    {
    "cleverLinks":    true,
    "monospaceLinks": true
  }
}
